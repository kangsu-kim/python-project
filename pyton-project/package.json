{
  "name": "cargo-transport-system",
  "version": "1.0.0",
  "description": "Web application for cargo transportation company",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "dev:all": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "npm run build --prefix client"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^7.0.1",
    "googleapis": "^126.0.1",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.2.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
